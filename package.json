{
  "name": "jsoft-agile",
  "version": "2.2.0",
  "description": "A Node.js module that provides a collection of ready-made functions used on large systems for their development.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "directories": {
    "dist": "dist"
  },
  "bin": {
    "jsoft-agile": "dist/index.js"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js --ignore __tests__",
    "test": "jest --watch",
    "publish:npm": "rm -rf dist && mkdir dist && sucrase ./src -d ./dist --transforms imports"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jaytrindade/jsoft-agile.git"
  },
  "preferGlobal": true,
  "keywords": [
    "jsoft",
    "agile",
    "npm",
    "lodash",
    "filters",
    "validates",
    "functions",
    "jsoft-agile",
    "nodejs agile",
    "ready",
    "template"
  ],
  "author": "Jo√£o Trindade <joaotrindade.soft@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jaytrindade/jsoft-agile/issues"
  },
  "homepage": "https://github.com/Jaytrindade/jsoft-agile#readme",
  "dependencies": {
    "mongoose": "^5.7.7"
  },
  "peerDependencies": {
    "mongoose": "^5.7.7"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "sucrase": "^3.10.1"
  }
}
